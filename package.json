{
   "name": "nextjs-blog-demo",
   "version": "0.1.0",
   "private": true,
   "scripts": {
      "dev": "concurrently \"next dev\" \"babel -w ./src --out-dir dist --extensions .ts,.tsx\"",
      "build": "next build",
      "start": "next start",
      "lint": "next lint",
      "typeorm": "typeorm-ts-node-commonjs",
      "typeorm:build": "babel ./src --out-dir dist --extensions \".ts,.tsx\"",
      "m:create": "typeorm migration:create",
      "m:run": "typeorm-ts-node-commonjs migration:run -d src/data-source.ts",
      "m:revert": "typeorm-ts-node-commonjs migration:revert -d src/data-source.ts",
      "e:create": "typeorm entity:create",
      "seed": "ts-node src/seed.ts"
   },
   "dependencies": {
      "@babel/cli": "^7.17.10",
      "@babel/core": "^7.17.10",
      "axios": "^0.27.2",
      "gray-matter": "^4.0.3",
      "iron-session": "^6.1.3",
      "lodash": "^4.17.21",
      "marked": "^4.0.14",
      "md5": "^2.3.0",
      "next": "12.1.5",
      "pg": "^8.4.0",
      "query-string": "^7.1.1",
      "react": "18.0.0",
      "react-dom": "18.0.0",
      "reflect-metadata": "^0.1.13",
      "typeorm": "0.3.6",
      "ua-parser-js": "^1.0.2"
   },
   "devDependencies": {
      "@babel/plugin-proposal-decorators": "^7.17.9",
      "@types/axios": "^0.14.0",
      "@types/lodash": "^4.14.182",
      "@types/marked": "^4.0.3",
      "@types/md5": "^2.3.2",
      "@types/node": "^16.11.10",
      "@types/react": "^18.0.5",
      "@types/react-dom": "^18.0.1",
      "@types/ua-parser-js": "^0.7.36",
      "concurrently": "^7.1.0",
      "eslint": "8.13.0",
      "eslint-config-next": "12.1.5",
      "sass": "^1.52.1",
      "ts-node": "10.7.0",
      "typescript": "4.5.2"
   }
}
