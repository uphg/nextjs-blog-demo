   },
   "dependencies": {
      "@babel/cli": "^7.17.10",
      "@babel/core": "^7.17.10",
      "axios": "^0.26.1",
      "gray-matter": "^4.0.3",
      "marked": "^4.0.14",
      "next": "12.1.5",
      "pg": "^8.4.0",
      "react": "18.0.0",
      "react-dom": "18.0.0",
      "reflect-metadata": "^0.1.13",
      "typeorm": "0.3.6",
      "ua-parser-js": "^1.0.2"
   },
   "devDependencies": {
      "@babel/plugin-proposal-decorators": "^7.17.9",
      "@types/axios": "^0.14.0",
      "@types/marked": "^4.0.3",
      "@types/node": "^16.11.10",
      "@types/react": "^18.0.5",
      "@types/react-dom": "^18.0.1",
      "@types/ua-parser-js": "^0.7.36",
      "eslint": "8.13.0",
      "eslint-config-next": "12.1.5",
